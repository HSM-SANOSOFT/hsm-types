{
  "name": "@hsm-sanosoft/hsm-types",
  "version": "0.0.1",
  "description": "Tipos compartidos y cat√°logos",
  "author": "Hospital Santamaria",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    }
  },
  "scripts": {
    "build:esm": "pnpm exec tsc -p tsconfig.esm.json",
    "build:cjs": "pnpm exec tsc -p tsconfig.cjs.json",
    "build": "pnpm run clean && pnpm run build:esm && pnpm run build:cjs",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@10.7.1",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "globals": "^16.3.0"
  }
}